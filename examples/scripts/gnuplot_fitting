f(x) = a + b*x*x+c*x*x*x*x
a=-2500.
b=-10000000.0
c=-10000000000.0
set fit errorvariables
#fit f(x) filename using 2:6:10 yerrors via a, b, c
fit f(x) filename using 2:5:9 yerrors via a, b, c
set print "fit_parameters.txt"
print a,a_err
print b,b_err
print c,c_err
set print
set xrange[0:]
set ylabel "Ground state energy (Kelvin)"
set xlabel "Imaginary time (tau) (Kelvin-1)"
#plot filename using 2:6 w lp, '' u 2:6:10 w yerr, f(x)
plot filename using 2:5 w lp, '' u 2:5:9 w yerr, f(x)
pause -1 "Hit Enter to continue" 

